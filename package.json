{
  "name": "orienteering-js",
  "version": "0.0.26",
  "description": "Utils for managing orienteering related data in javascript",
  "files": [
    "dist/",
    "types/",
    "!tests/",
    "!src/"
  ],
  "type": "module",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "build": "tsc"
  },
  "keywords": [
    "orienteering",
    "javascript"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./gps-seuranta": {
      "types": "./types/gps-seuranta/index.d.ts",
      "default": "./dist/gps-seuranta/index.js"
    },
    "./loggator": {
      "types": "./types/loggator/index.d.ts",
      "default": "./dist/loggator/index.js"
    },
    "./map": {
      "types": "./types/map/index.d.ts",
      "default": "./dist/map/index.js"
    },
    "./models": {
      "types": "./types/models/index.d.ts",
      "default": "./dist/models/index.js"
    },
    "./ocad": {
      "types": "./types/ocad/index.d.ts",
      "default": "./dist/ocad/index.js"
    },
    "./routechoice-detector": {
      "types": "./types/routechoice-detector/index.d.ts",
      "default": "./dist/routechoice-detector/index.js"
    },
    "./split-times": {
      "types": "./types/split-times/index.d.ts",
      "default": "./dist/split-times/index.js"
    },
    "./statistics": {
      "types": "./types/statistics/index.d.ts",
      "default": "./dist/statistics/index.js"
    },
    "./two-d-rerun": {
      "types": "./types/two-d-rerun/index.d.ts",
      "default": "./dist/two-d-rerun/index.js"
    },
    "./utils": {
      "types": "./types/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "author": "Nicolas Rio",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "linkedom": "^0.15.3",
    "prettier": "3.0.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "mathjs": "^11.11.0"
  },
  "peerDependencies": {
    "zod": "^3.21.4"
  }
}
